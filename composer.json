{
    "name": "shiftonelabs/laravel-nomad",
    "description": "Add extra functionality to Laravel migrations.",
    "keywords": ["laravel", "migration", "custom"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/shiftonelabs/laravel-nomad/issues",
        "source": "https://github.com/shiftonelabs/laravel-nomad"
    },
    "authors": [
        {
            "name": "Patrick Carlo-Hickman",
            "email": "patrick@shiftonelabs.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/database": ">=4.0",
        "illuminate/support": ">=4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "laravel/lumen-framework": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "ShiftOneLabs\\LaravelNomad\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShiftOneLabs\\LaravelNomad\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
